<h1>Models Grid</h1>
<% for model in @models %>

  <div class="span4 device">
    <div class="device_inner">
        <div class="image_wrapper"><img src="images/galaxys3.png" alt="Samsung Galaxy S3"/></div>
        <div class="os_type" id="os_android"></div>
    </div>
    <div class="make_and_model">
        <span class="manufacturer">Samsung</span>
        <h3 class="model">Galaxy S3</h3>
    </div>
    <div class="availability available">
        <div>
            4<br>
            <span>Available</span>
        </div>
    </div>
  </div>
  
<% end %>