
<h1>Users</h1>
  <table class="table">
  <thead>
    <th>ID</th>
    <th>AndrewID</th>
    <th>Email</th>
    <th>Role</th>
    <th>&nbsp;</th>
  </thead>
    <% for user in @users %>
      <tr>
        <td><%= user.id %></td>
        <td><%= link_to user.andrew, user_path(user) %></td>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td>
          <%= link_to("Edit", edit_user_path(user))  %>&nbsp;
          <%= link_to("Delete", user_path(user), :confirm => 'Are you sure you wish to delete this information?', :method => :delete) %>
        </td>
      </tr>
  <% end %>
</table>
<%= link_to "Create User", new_user_path, :class => "button button-blue"  %>